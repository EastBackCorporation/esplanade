<p id="notice"><%= notice %></p>

<div  class="container">
  <table class="table table-striped">
    <tbody>
      <% @category.topics.each do |t| %>
      <tr>
        <td>
        <%= link_to t do %>
        <div>
          <strong><%= t.title %></strong>
        </div>
        <div>
          <label class="long-text"><%= t.value %></label>
        </div>
        <% end %>
        </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>

